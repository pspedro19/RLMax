# MinIO Configuration (for connecting to pipeline data buckets)
NEXT_PUBLIC_S3_ENDPOINT=http://localhost:9000
NEXT_PUBLIC_S3_ACCESS_KEY=airflow
NEXT_PUBLIC_S3_SECRET_KEY=airflow

# TwelveData API Keys (for real-time data and monitoring)
NEXT_PUBLIC_TWELVEDATA_API_KEY_1=your_api_key_1_here
NEXT_PUBLIC_TWELVEDATA_API_KEY_2=your_api_key_2_here
NEXT_PUBLIC_TWELVEDATA_API_KEY_3=your_api_key_3_here
NEXT_PUBLIC_TWELVEDATA_API_KEY_4=your_api_key_4_here
NEXT_PUBLIC_TWELVEDATA_API_KEY_5=your_api_key_5_here
NEXT_PUBLIC_TWELVEDATA_API_KEY_6=your_api_key_6_here
NEXT_PUBLIC_TWELVEDATA_API_KEY_7=your_api_key_7_here
NEXT_PUBLIC_TWELVEDATA_API_KEY_8=your_api_key_8_here

# WebSocket Configuration (optional - for real-time data streaming)
NEXT_PUBLIC_WS_ENDPOINT=wss://ws.twelvedata.com/v1/quotes/price

# Development Configuration
NODE_ENV=development
NEXT_TELEMETRY_DISABLED=1